#!/bin/bash
# copy scenario files generated by local cml to cluster cml
# copy user-generated parity check matrices to cluster

# path to local scenarios
PLS=/home/tferrett/cml/localscenarios

# path to remote scenarios
PRS=wcrlcluster.csee.wvu.edu:/home/tferrett/cml/

# copy local scenarios to cluster
scp -r $PLS $PRS



# path to local parity check matrices
PLD=/home/tferrett/cml/data/ldpc/*

# path to remote PCMs
PRD=wcrlcluster.csee.wvu.edu:/home/tferrett/cml/data/ldpc

# copy local scenarios to cluster
#scp -r $PLD $PRD



# path to pcs PCM's
PPCSD=pcs@wcrlcluster.csee.wvu.edu:/home/pcs/projects/cml/data/ldpc

# copy local parity check matrices to cluster
#scp -r $PLD $PPCSD

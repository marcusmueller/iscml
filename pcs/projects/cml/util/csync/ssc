#!/bin/bash
# copy scenario files generated by local cml to cluster cml

# path to local scenarios
PLS=/home/tferrett/cmlmerge/cmlnew/localscenarios

# path to remote scenarios
PRS=wcrlcluster.csee.wvu.edu:/home/tferrett/cml/

# copy local scenarios to cluster
scp -r $PLS $PRS


# path to permanent scenarios
PLPS=/home/tferrett/cmlmerge/cmlnew/scenarios

# copy permanent scenarios to cluster
scp -r $PLPS $PRS

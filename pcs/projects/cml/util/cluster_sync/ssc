#!/bin/bash
# push scenario files generated by local cml to cluster cml

# path to local scenarios
PLS=/home/tferrett/cmlmerge/cmlnew/localscenarios

# path to remote scenarios
PRS=wcrlcluster.csee.wvu.edu:/home/tferrett/cml/

# send scenarios to cluster
scp -r $PLS $PRS


# path to scenarios
PLAS=/home/tferrett/cmlmerge/cmlnew/scenarios

# path to remote scenarios
PCLS=wcrlcluster.csee.wvu.edu:/home/tferrett/cml/

# send scenarios to cluster
scp -r $PLAS $PCLS
